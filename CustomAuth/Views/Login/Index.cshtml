@model CustomAuth.Models.Usuario

@{
    ViewBag.Title = "Index";
}


@using (Html.BeginForm())
{
    <div class="container col-lg-offset-4 col-lg-4">
        @Html.LabelFor(o => o.Login)
        @Html.TextAreaFor(o => o.Login)
        @Html.ValidationMessageFor(o => o.Login)
        <br />
        @Html.LabelFor(o => o.Password)
        @Html.TextAreaFor(o => o.Password)
        @Html.ValidationMessageFor(o => o.Password)
        <br />


        <button type="submit"><i class="glyphicon glyphicon-log-in"></i>Enviar</button>
    </div>

}



